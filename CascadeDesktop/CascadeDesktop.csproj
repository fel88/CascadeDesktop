<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0-windows7.0</TargetFramework>
    <OutputType>WinExe</OutputType>
    <UseWindowsForms>true</UseWindowsForms>
    <UseWPF>true</UseWPF>
    <ImportWindowsDesktopTargets>true</ImportWindowsDesktopTargets>
    <ApplicationIcon>cube.ico</ApplicationIcon>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <PlatformTarget>x64</PlatformTarget>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="fonts\Antonio-Bold.ttf" />
    <None Remove="fonts\Antonio-Light.ttf" />
    <None Remove="fonts\Antonio-Regular.ttf" />
    <None Remove="fonts\OCRAEXT.TTF" />
    <None Remove="Icons\application-blue.png" />
    <None Remove="Icons\block.png" />
    <None Remove="shaders\cam_space_shader.fs" />
    <None Remove="shaders\cam_space_shader.vs" />
    <None Remove="shaders\text.fs" />
    <None Remove="shaders\text.vs" />
  </ItemGroup>
  <ItemGroup>
